+import { supabase } from './supabaseClient.js'
+
+async function ensureAuthRedirect(){
+  const { data } = await supabase.auth.getUser()
+  if (!data.user) { location.href = '/login.html'; return null }
+  // check role
+  const { data: profile } = await supabase.from('profiles').select('role').eq('id', data.user.id).single().catch(()=>({data:null}))
+  if (profile?.role !== 'admin'){ alert('Not authorized'); location.href = '/login.html' }
+  return data.user
+}
+
+async function loadContractors(){
+  const user = await ensureAuthRedirect(); if(!user) return
+  const { data: contractors } = await supabase.from('profiles').select('*').neq('role','admin')
+  const container = document.getElementById('contractors-list')
+  container.innerHTML = ''
+  contractors.forEach(c=>{
+    const d = document.createElement('div'); d.className='lead-item'
+    d.innerHTML = `<strong>${c.company_name || c.contact_name || c.email}</strong><div>Service: ${c.main_service||'—'}</div><div>Chatbot: ${c.chatbot_id? '✓':'—'}</div>`
+    container.appendChild(d)
+  })
+}
+
+async function loadAllLeads(){
+  await ensureAuthRedirect()
+  const { data: leads } = await supabase.from('leads').select('*, profiles:profiles(id,company_name,contact_name)').order('created_at',{ascending:false})
+  const container = document.getElementById('all-leads-list')
+  container.innerHTML = ''
+  leads.forEach(l=>{
+    const div = document.createElement('div'); div.className='lead-item'
+    const by = l.profiles?.company_name || l.profiles?.contact_name || 'Unknown'
+    div.innerHTML = `<strong>${l.name||l.email||l.phone}</strong><div>${l.message}</div><small>Assigned to: ${by}</small>`
+    container.appendChild(div)
+  })
+}
+
+document.getElementById('btn-refresh-contractors').addEventListener('click', loadContractors)
+
+loadContractors(); loadAllLeads()
+
+// realtime
+supabase.channel('public:leads')
+  .on('postgres_changes', { event: 'INSERT', schema: 'public', table: 'leads' }, payload=>{
+    loadAllLeads(); loadContractors()
+  }).subscribe()
+
