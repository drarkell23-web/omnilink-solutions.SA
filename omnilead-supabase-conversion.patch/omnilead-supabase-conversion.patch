+import { supabase } from './supabaseClient.js'
+
+async function ensureAuthRedirect(){
+  const { data } = await supabase.auth.getUser()
+  if (!data.user) { location.href = '/login.html'; return null }
+  return data.user
+}
+
+async function loadProfile(){
+  const user = await ensureAuthRedirect(); if(!user) return
+  const { data: profile } = await supabase.from('profiles').select('*').eq('id', user.id).single().catch(()=>({data:null}))
+  document.getElementById('company_name').value = profile?.company_name || ''
+  document.getElementById('contact_name').value = profile?.contact_name || ''
+  document.getElementById('phone').value = profile?.phone || ''
+  document.getElementById('main_service').value = profile?.main_service || ''
+  document.getElementById('chatbot_id').value = profile?.chatbot_id || ''
+  document.getElementById('chatbot_token').value = profile?.chatbot_token || ''
+}
+
+async function saveProfile(){
+  const user = await ensureAuthRedirect(); if(!user) return
+  const payload = {
+    id: user.id,
+    company_name: document.getElementById('company_name').value,
+    contact_name: document.getElementById('contact_name').value,
+    phone: document.getElementById('phone').value,
+    main_service: document.getElementById('main_service').value
+  }
+  await supabase.from('profiles').upsert(payload)
+  alert('Profile saved')
+}
+
+async function saveChatbot(){
+  const user = await ensureAuthRedirect(); if(!user) return
+  const chatbot_id = document.getElementById('chatbot_id').value
+  const chatbot_token = document.getElementById('chatbot_token').value
+  await supabase.from('profiles').upsert({ id: user.id, chatbot_id, chatbot_token })
+  alert('Chatbot credentials saved')
+}
+
+async function loadLeads(){
+  const user = await ensureAuthRedirect(); if(!user) return
+  // leads table should contain: id, contractor_id, name, phone, email, message, created_at
+  const { data: leads } = await supabase.from('leads').select('*').eq('contractor_id', user.id).order('created_at', {ascending:false})
+  const list = document.getElementById('leads-list')
+  const select = document.getElementById('lead-select')
+  list.innerHTML = ''
+  select.innerHTML = ''
+  if (!leads || leads.length===0){ list.innerHTML = '<p>No leads yet</p>'; return }
+  leads.forEach(l=>{
+    const div = document.createElement('div'); div.className='lead-item'
+    div.innerHTML = `<strong>${l.name||l.email||l.phone}</strong><div>${l.message||''}</div><small>${new Date(l.created_at).toLocaleString()}</small>`
+    list.appendChild(div)
+    const opt = document.createElement('option'); opt.value = l.id; opt.textContent = l.name || l.email || l.phone
+    select.appendChild(opt)
+  })
+}
+
+async function sendMessage(){
+  const user = await ensureAuthRedirect(); if(!user) return
+  const leadId = document.getElementById('lead-select').value
+  const text = document.getElementById('message-text').value
+  if (!leadId || !text) return alert('Select lead and enter message')
+  // fetch lead and contractor chatbot creds
+  const { data: lead } = await supabase.from('leads').select('*').eq('id', leadId).single()
+  const { data: contractor } = await supabase.from('profiles').select('chatbot_id,chatbot_token,email').eq('id', user.id).single()
+
+  // store message in messages table for audit
+  await supabase.from('messages').insert([{ lead_id: leadId, contractor_id: user.id, text }])
+
+  // Forward message to contractor's chatbot if credentials exist
+  if (contractor?.chatbot_id && contractor?.chatbot_token){
+    try{
+      // Example HTTP POST to chatbot endpoint — update according to your chatbot API
+      await fetch(`https://api.chatbot.example/send`, {
+        method:'POST',
+        headers:{ 'Authorization': `Bearer ${contractor.chatbot_token}`, 'Content-Type':'application/json' },
+        body: JSON.stringify({ to: contractor.chatbot_id, message: text, lead })
+      })
+    }catch(e){ console.error('chatbot send error', e) }
+  }
+
+  // also notify admin by creating an admin notification
+  await supabase.from('admin_notifications').insert([{ contractor_id: user.id, lead_id: leadId, text }])
+
+  alert('Message sent')
+}
+
+// wire events
+document.getElementById('btn-save-profile').addEventListener('click', saveProfile)
+document.getElementById('save-chatbot').addEventListener('click', saveChatbot)
+document.getElementById('btn-send-message').addEventListener('click', sendMessage)
+
+// init
+loadProfile(); loadLeads()
+
+// realtime listener for new leads on this contractor
+(async ()=>{
+  const user = await ensureAuthRedirect(); if(!user) return
+  supabase.channel('public:leads')
+    .on('postgres_changes', { event: 'INSERT', schema: 'public', table: 'leads', filter: `contractor_id=eq.${user.id}` }, payload=>{
+      console.log('new lead', payload)
+      loadLeads()
+    }).subscribe()
+})()
+
